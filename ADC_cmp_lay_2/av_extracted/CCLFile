capacitance \
	 -decoupling_factor 1.0 \
	 -ground_net "gnd!"
extract \
	 -selection "all" \
	 -type "c_only_coupled"
extraction_setup \
	 -macro_cells_type "default" \
	 -net_name_space "LAYOUT"
filter_coupling_cap \
	 -coupling_cap_threshold_absolute 0.01 \
	 -coupling_cap_threshold_relative 0.001
input_db -type assura \
	 -design_cell_name "ADC_cmp_lay3 layout TERASCALE" \
	 -directory_name "/home/iperic/terascale2/ASSURA_LVS" \
	 -format "DFII" \
	 -run_name "ADC_cmp_lay3"
log_file \
	 -file_name "/home/iperic/terascale2/ASSURA_LVS/qrc.ADC_cmp_lay3.log"
output_db -type extracted_view \
	 -cap_component "pcapacitor" \
	 -cap_property_name "c" \
	 -enable_cellview_check false \
	 -include_cap_model "false" \
	 -include_parasitic_cap_model "false" \
	 -include_parasitic_res_model "comment" \
	 -include_res_model "false" \
	 -view_name "av_extracted"
output_setup \
	 -directory_name "/home/iperic/terascale2/ASSURA_LVS" \
	 -temporary_directory_name "ADC_cmp_lay3"
process_technology \
	 -technology_corner \
		"Typical" \
	 -technology_library_file "/home/iperic/terascale2/assura_tech.lib" \
	 -technology_name "h18a7am"
extract \
	 -field_solver_type "deterministic" \
	 -selection "all" \
	 -type "c_only_coupled" \
	 -use_field_solver "default_accuracy"


